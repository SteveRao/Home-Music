html,body{
	margin: 0px;
	padding: 0px;
	height: 100%;
}

#main-container{
	width: 60%;
	height: 100%;
	background-color:burlywood;
	margin: auto;
}

#player-container{
	width: 180px;
	height:180px;
	background-color:azure;
	margin: auto;
	border-radius: 50%;
}

#player-wrap{
	position: relative;
}

.navigation{
	width: 110px;
	height: 110px;
	position: absolute;
	background-color: azure;
	border-radius: 50%;
	line-height: 110px;
	text-align: center;
	cursor: pointer;
	transition: background-color 1s;
	
	margin-left: 35px;
	margin-top: 35px;
}

.player-btn{
	width: 60px;
	height: 60px;
	position: absolute;
	background-color: azure;
	border-radius: 50%;
	line-height: 110px;
	text-align: center;
	cursor: pointer;
	transition: background-color 1s;
	
	margin-left: 60px;
	margin-top: 60px;
}

.navigation:hover
{
		background-color: #1B6D85;
}

.effect{
	position: absolute;
	width: 110px;
	height: 110px;
	background-color: #5E5E5E;
	border-radius: 50%;
	transition: all 0.4s;
	
}
.effect:hover{
	transform: scale(0.9, 0.9);
}

.player-effect{
	position: absolute;
	width: 60px;
	height: 60px;
	background-color:#2AABD2;
	border-radius: 50%;
	transition: all 0.4s;
	line-height: 60px;
}

.player-effect:hover{
	transform: scale(0.9, 0.9);
}

.box-1{
	transform: translate(198px, -198px);
	font: "微软雅黑";
	font-size: 1.8em;
}

.box-2{
	transform: translate(0px, -280px);
	font: "微软雅黑";
	font-size: 2em;
}

.box-3{
	transform: translate(-198px, -198px);
	font: "微软雅黑";
	font-size: 1.8em;
}

.box-4{
	transform: translate(-280px, 0px);
	font: "微软雅黑";
	font-size: 1.8em;
}

.box-5{
	transform: translate(-198px, 198px);
	font: "微软雅黑";
	font-size: 2em;
}

.box-6{
	transform: translate(0px, 280px);
	font: "微软雅黑";
	font-size: 2em;
}

/*播放器功能按钮*/

.box-7{
	transform: translate(160px, 0px);
}

.box-8{
	transform: translate(139px, 80px);
}

.box-9{
	transform: translate(80px, 139px);
}

.box-10{
	transform: translate(0px, 160px);
}

.box-11{
	transform: translate(-160px, 0px);
}

.box-12{
	transform: translate(0px, -160px);
}


/*弹出框*/

.flip-container{
	position: relative;
	width: 700px;
	height: 700px;
	background:rgba(225, 225, 225, 0.3);
	left: -260px;
	top: -260px;
	display: none;
}

.special-wrap{
	position: relative;
	width: 400px;
	height: 600px;
	background-color: #9D9D9D;
	left: 150px;
	top: 30px;
}

.title{
	width: 100%;
	height: 60px;
	background-color: #1B6D85;
	font: "微软雅黑";
	font-size: 2.5em;
	line-height: 60px;
	text-align: center;
}

.mana-title{
	width: 100%;
	height: 40px;
	background-color: #1B6D85;
	font: "微软雅黑";
	font-size: 1.5em;
	line-height: 40px;
}

.scl-item{
	height: 50px;
	line-height: 50px;
	vertical-align: middle;
}
tbody td{
	vertical-align: middle;
	
}

.page{
	width: 100%;
}

.close-flip{
	width: 20px;
	height: 20px;
	background-color: #AFD9EE;
	position: relative;
	margin-left: 680px;
	line-height: 20px;
	text-align: center;
	border-radius: 50px;
	opacity: 0.3;
	transition: opacity 0.3s;
	cursor: pointer;
}
.close-flip:hover{
	opacity: 1;
}


/*搜索框*/
.search-wrap{
	width: 100%;
	height: 60px;
	background-color: #1B6D85;
}

.sr-style{
	position: relative;
	width: 300px;
	margin: auto;
	top: 13px;
}


.lst-title{
	margin-left: 10px;
}

.lst-create{
	margin-left:180px;
}

.lst-item{
	cursor: pointer;
}

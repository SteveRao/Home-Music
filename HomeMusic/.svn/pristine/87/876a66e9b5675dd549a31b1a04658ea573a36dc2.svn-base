	border-top-right-radius: 5px;
}

.scl-item{
	height: 50px;
	line-height: 50px;
	vertical-align: middle;
	position: relative;
}
tbody td{
	vertical-align: middle;
	
}

.page{
	width: 100%;
}

.close-flip{
	width: 20px;
	height: 20px;
	background-color: #AFD9EE;
	position: relative;
	margin-left: 680px;
	line-height: 20px;
	text-align: center;
	border-radius: 50px;
	opacity: 0.3;
	transition: opacity 0.3s;
	cursor: pointer;
}
.close-flip:hover{
	opacity: 1;
}


/*搜索框*/
.search-wrap{
	width: 100%;
	height: 60px;
	background-color: #1B6D85;
}

.sr-style{
	position: relative;
	width: 300px;

-- ----------------------------
-- Table structure for employee
-- ----------------------------
DROP TABLE IF EXISTS `employee`;
CREATE TABLE `employee` (
  `id` int(11) NOT NULL,
  `post_type` int(11) NOT NULL,
  `emp_name` varchar(100) DEFAULT NULL,
  `emp_sex` int(11) DEFAULT NULL,
  `emp_age` int(11) DEFAULT NULL,
  `emp_deptart` varchar(50) DEFAULT NULL,
  `emp_year` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `fk1` (`post_type`),
  KEY `fk2` (`emp_sex`),
  CONSTRAINT `employee_ibfk_1` FOREIGN KEY (`post_type`) REFERENCES `post` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `employee_ibfk_2` FOREIGN KEY (`emp_sex`) REFERENCES `sex` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

